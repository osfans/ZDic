INFORMATION
BEGIN
     TITLE "Dictionary Not Found"
     MESSAGE "Dictionary not installed."
     BUTTONS "OK" 
END

ALERT ID NoEnoughMemoryAlert 
WARNING
BEGIN
     TITLE "Alert"
     MESSAGE "Not Enough Memory!"
     BUTTONS "OK" 
END
 
ALERT ID NoFontAlert 
WARNING
BEGIN
     TITLE "Alert"
     MESSAGE "ZDic font share library not found!"
     BUTTONS "OK" 
END

