		MENUITEM SEPARATOR ID 10005
		MENUITEM "Keyboard" ID 10006 "K"
		MENUITEM "Graffiti Help" ID 10007 "G"
    END

	PULLDOWN "Options"
	BEGIN
		MENUITEM "Preferences... " ID OptionsPreferences "R"
		MENUITEM "Dict Options... " ID OptionsDictOptions "D"
